{
  "private": true,
  "name": "skyway-conference-dev",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run dev:js & npm run dev:css & wait",
    "dev:js": "webpack-dev-server --mode development",
    "dev:css": "node ./script/postcss-multi.js -w",
    "build": "npm run build:js && npm run build:css",
    "build:js": "NODE_ENV=production webpack --mode production",
    "build:css": "NODE_ENV=production node ./script/postcss-multi.js",
    "lint": "eslint . --fix --ext .js --ext .jsx",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "author": "",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cssnano": "^4.1.7",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.7.0",
    "jasmine": "^3.1.0",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^6.0.0",
    "postcss-cli": "^6.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "prettier": "^1.11.1",
