    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "mobx": "^5.0.0",
    "mobx-react": "^5.0.0",
    "mousetrap": "^1.6.1",
    "normalize.css": "^8.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "voice-activity-detection": "0.0.5"
  }
