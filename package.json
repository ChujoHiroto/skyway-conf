{
  "private": true,
  "name": "skyway-conference-dev",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run dev:js & npm run dev:css & wait",
    "dev:js": "webpack-dev-server --mode development",
    "dev:css": "node ./script/postcss-multi.js -w",
    "build": "npm run build:js && npm run build:css",
    "build:js": "NODE_ENV=production webpack --mode production",
    "build:css": "NODE_ENV=production node ./script/postcss-multi.js",
    "lint": "eslint . --fix --ext .js --ext .jsx",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "author": "",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jasmine": "^3.4.0",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.0",
    "postcss-cli": "^6.1.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.17.0",
